  {
    "name": "Word Cloud",
    "description" : "Word Cloud creator for .csv files",
    "version": "0.1",
    "manifest_version": 2,
	"icons": {
		"16": "16.png",
		"32": "32.png",
		"48": "48.png",
		"128": "128.png"
	},
	"browser_action":{
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"tabs", 
        "<all_urls>"
	],
	"content_scripts":[
    {
      "matches": ["<all_urls>"], 
      "js": ["get_text.js"]
    }
    ] 

  }
  
  
